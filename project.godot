"language": "GDScript",
"path": "res://BaseClasses/ButtonBase.gd"
}, {
"base": "Node",
"class": "InputsBase",
"language": "GDScript",
"path": "res://BaseClasses/InputsBase.gd"
}, {
"base": "PlayerStateBase",
"class": "MoveBase",
"language": "GDScript",
"path": "res://BaseClasses/MoveBase.gd"
}, {
"base": "StateBase",
"class": "PlayerStateBase",
"language": "GDScript",
"path": "res://BaseClasses/PlayerStateBase.gd"
}, {
"base": "Node",
"class": "StateBase",
"language": "GDScript",
"path": "res://BaseClasses/StateBase.gd"
}, {
"base": "Node",
